{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "app/utils/Links.js", "app/components/home/MediaCard.jsx", "../old/middleware.ts", "../old/middleware.ts", "app/components/shared/swiper/Swiper.jsx", "app/components/home/HashCarousel.jsx"],
  "exclude": ["node_modules"],
  "resolve": {
    "alias": {
      "@mui/material": "@mui/joy"
    }
  }
}
